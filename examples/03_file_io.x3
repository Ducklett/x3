import "./lib/std"

use std

#entrypoint
proc main[] {
    const file content = "Demo file contents:
(This file was generated by 03_file_io.x3)

- foo
- bar
- baz
";

    write all text(c"out.txt", file content);
    append text(c"out.txt", "- qux\n");
    append text(c"out.txt", "- quuuuux\n");

    assert(10>20);
    exit(0);
}
