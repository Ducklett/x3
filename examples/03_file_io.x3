import "./lib/standard" use standard

proc main {
	const file content = "Demo file contents:
(This file was generated by 03_file_io.x3)

- foo
- bar
- baz
"

	write_all_text(c"out.txt", file content)
	append_text(c"out.txt", "- qux\n")
	append_text(c"out.txt", "- quuuuux\n")

	write_line("==================")
	write_line("done writing file!")
	write_line("==================")
}
