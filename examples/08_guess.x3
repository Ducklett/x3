import "./lib/standard" use standard

proc main {
    var number = random range(0,100)
    var guessbuffer:[5]char

    while true {
        write("Please guess the number:")

        var guess = read(guessbuffer) => parse int

		match guess {
        	guess > number' write line("Lower")
			guess < number' write line("Higher")
			else {
				write line("You got it!")
				break
			}
		}
    }

    write line("See ya!")
}
